{
  "name": "svgmap",
  "description": "svgMap is a JavaScript library that lets you easily create an interactable world map comparing customizable data for each country.",
  "version": "2.18.1",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "svg-map",
    "world-map",
    "chart",
    "countries"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stephanwagner/svgMap.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "style": "./dist/svgmap.css",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "svg-pan-zoom": "^3.6.2"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "prepublishOnly": "npm run build && node test/assets.js"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "csso": "^5.0.5",
    "rollup": "^4.54.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.97.1"
  }
}
